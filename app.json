{
    "expo": {
        "name": "Boiler Expo App",
        "slug": "boiler-expo",
        "version": "1.0.0",
        "orientation": "default",
        "userInterfaceStyle": "light",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#100D25"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.zetasolutionsonline.zetapos",
            "supportsTablet": true,
            "infoPlist": {
                "CFBundleAllowMixedLocalizations": true
            }
        },
        "android": {
            "softwareKeyboardLayoutMode": "resize",
            "package": "com.zetasolutionsonline.zetapos",
            "jsEngine": "hermes"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["sentry-expo"]
    }
}
